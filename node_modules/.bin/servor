#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/renshuai/Documents/脚手架/vite/webAssembly-js-vite/node_modules/.pnpm/servor@4.0.2/node_modules/servor/node_modules:/Users/renshuai/Documents/脚手架/vite/webAssembly-js-vite/node_modules/.pnpm/servor@4.0.2/node_modules:/Users/renshuai/Documents/脚手架/vite/webAssembly-js-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/renshuai/Documents/脚手架/vite/webAssembly-js-vite/node_modules/.pnpm/servor@4.0.2/node_modules/servor/node_modules:/Users/renshuai/Documents/脚手架/vite/webAssembly-js-vite/node_modules/.pnpm/servor@4.0.2/node_modules:/Users/renshuai/Documents/脚手架/vite/webAssembly-js-vite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../servor/cli.js" "$@"
else
  exec node  "$basedir/../servor/cli.js" "$@"
fi
